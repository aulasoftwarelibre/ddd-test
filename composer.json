{
    "name": "aulasoftwarelibre/ddd-test",
    "description": "Base test classes for DDD projects",
    "authors": [
        {
            "name": "Sergio Gómez",
            "email": "sergio@uco.es"
        },
        {
            "name": "Víctor Monserrat",
            "email": "victormonserratvillatoro@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-mbstring": "*",
        "coduo/php-matcher": "^3.1",
        "doctrine/collections": "^1.5",
        "phpspec/php-diff": "^1.1",
        "prooph/common": "^4.2",
        "prooph/event-sourcing": "^5.6",
        "prooph/event-store": "^7.3",
        "prooph/event-store-symfony-bundle": "^0.5",
        "symfony/browser-kit": "^4.1",
        "symfony/config": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/http-foundation": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/messenger": "^4.2",
        "webmozart/assert": "^1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AulaSoftwareLibre\\DDD\\TestsBundle\\": "src/"
        }
    }
}
